<!DOCTYPE html>
<html ng-app="ReviewsJoy" ng-cloak>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>ReviewsJoy</title>
    @Styles.Render("~/Content/css")
    @Scripts.Render("~/bundles/modernizr")

    <!-- Google Maps -->
    <script src='//maps.googleapis.com/maps/api/js?sensor=false'></script>
</head>
<body @*style="background-image: url('/Content/photo-1428908728789-d2de25dbd4e2.jpg'); background-size: 100% 100%;background-repeat: no-repeat;"*@>
    <div class="container">
        @RenderBody()
    </div>
    <!-- Angular JS -->
    <script src="https://ajax.googleapis.com/ajax/libs/angularjs/1.3.14/angular.min.js"></script>
    <script src="https://ajax.googleapis.com/ajax/libs/angularjs/1.2.1/angular-route.min.js"></script>
    <!-- Angular JS end-->

    @Scripts.Render("~/bundles/jquery")

    <!-- Bootstrap -->
    <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.5/css/bootstrap.min.css">
    <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.5/css/bootstrap-theme.min.css">
    <script src="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.5/js/bootstrap.min.js"></script>
    <!-- Bootstrap end -->

    <!-- Jquery UI -->
    <script src="~/scripts/jquery-ui.min.js"></script>
    <link href="~/content/jquery-ui.min.css" rel="stylesheet" />
    <!-- Jquery UI end-->

    <!-- angular-google-maps -->
    <script src="~/Scripts/loaddash.js"></script>
    <script src="~/Scripts/angular-google-maps.js"></script>
    <!-- angular-google-maps end -->
    @RenderSection("scripts", required: false)
</body>
</html>
